{
  "expo": {
    "name": "Arc8",
    "slug": "arc8",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "arc8",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourdomain.arc8",
      "buildNumber": "1.0.0",
      "config": {
        "usesNonExemptEncryption": false
      },
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourdomain.arc8",
      "versionCode": 1,
      "permissions": [],
      "newArchEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ],
      "expo-font",
      "react-native-reanimated"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "fonts": [
      {
        "asset": "assets/fonts/DMSans-Regular.ttf",
        "family": "DMSans-Regular"
      },
      {
        "asset": "assets/fonts/DMSans-Medium.ttf",
        "family": "DMSans-Medium"
      },
      {
        "asset": "assets/fonts/DMSans-Bold.ttf",
        "family": "DMSans-Bold"
      },
      {
        "asset": "assets/fonts/DMSans-Italic.ttf",
        "family": "DMSans-Italic"
      },
      {
        "asset": "assets/fonts/FunnelSans-Regular.ttf",
        "family": "FunnelSans-Regular"
      },
      {
        "asset": "assets/fonts/FunnelSans-Medium.ttf",
        "family": "FunnelSans-Medium"
      },
        {
          "asset": "assets/fonts/FunnelSans-Bold.ttf",
          "family": "FunnelSans-Bold"
        },
        {
          "asset": "assets/fonts/FunnelSans-BoldItalic.ttf",
          "family": "FunnelSans-BoldItalic"
        }
    ]
  }
}
